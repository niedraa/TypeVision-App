{
  "expo": {
    "name": "TypeVision",
    "slug": "typevision",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"],
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "splash": {
      "backgroundColor": "#F5F5F5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Cette application a besoin d'accéder à votre galerie photo pour vous permettre de changer votre photo de profil."
      }
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE"
      ]
    }
  }
}
